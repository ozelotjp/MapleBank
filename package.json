{
  "name": "maplebank",
  "version": "1.0.0",
  "description": "My gnarly Nuxt.js project",
  "author": "ozelotjp",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "!": "yarn run lint --fix && git add . && git commit -m update && git push origin master"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@nuxtjs/pwa": "^3.0.0-0",
    "@nuxtjs/vuetify": "^1.8.5",
    "firebase": "^6.6.2",
    "nuxt": "^2.9.2",
    "pug": "^2.0.4",
    "pug-plain-loader": "^1.0.0",
    "vue-sweetalert2": "^2.1.1",
    "vuexfire": "^3.1.2"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^1.1.2",
    "@nuxtjs/eslint-module": "^1.1.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.4.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-nuxt": ">=0.4.3",
    "eslint-plugin-prettier": "^3.1.1",
    "husky": "^3.0.5",
    "lint-staged": "^9.2.5",
    "prettier": "^1.18.2"
  }
}
